{
  "name": "botframework-webchat",
  "version": "0.10.6",
  "description": "Embeddable web chat control for the Microsoft Bot Framework",
  "main": "js/BotChat.js",
  "scripts": {
    "build-css": "node-sass ./src/scss/ -o ./css",
    "build-css-watch": "node-sass ./src/scss/ -o ./css -w",
    "build": "npm run build-css",
    "start": "http-server -p 8000"
  },
  "repository": {
    "type": "git",
    "url": "git://git@github.com/Microsoft/BotFramework-WebChat.git"
  },
  "author": "Microsoft Corp",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^0.9.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "remote": "^0.2.6",
    "source-map-loader": "^0.2.1"
  }
}
